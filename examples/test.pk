extern puts(x:int, y:int) end
extern puti(num:int) end
extern putf(num:float) end

MyConst: int : 10 2 +;

proc add(x: int, y: int) int do
    return x y +
end

proc add_1_f(x:float) float do
    return x 1.0 +
end

pub proc main() do
    i: int = 0;
    while i < 10 do
        puti(i)
        i = add(i, 1);
    end
    z: float = 10.2;
    putf(add_1_f(z))
end