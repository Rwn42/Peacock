extern puts(x:int, y:int) end
extern puti(num:int) end
extern putf(num:float) end


memory arr 100 end

proc fill_array(amount: int, value: int) do
    i: int = 0;
    while i < amount do
        arr[i] = value;
        i = i 1 +;
    end
end


pub proc main() do
    fill_array(30, 40)
    i: int = 0;
    while i < 30 do
        puti(arr[i])
        i = i 1 +;
    end
end